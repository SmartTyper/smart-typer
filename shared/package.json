{
  "name": "shared",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "scripts": {
    "lint:ts": "npx eslint \"src/**/*.ts\"",
    "lint": "npm run lint:ts",
    "build:ts": "tsc && tscpaths -p tsconfig.json --src src --out build",
    "build": "npm run build:ts && cp package.json package-lock.json build"
  },
  "devDependencies": {
    "tscpaths": "0.0.9",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "joi": "17.7.0"
  }
}
